#!/bin/sh

# params:
# java -cp ./target/deeplearning4j-cli-api-0.0.3.3.3.alpha1.jar org.deeplearning4j.cli.driver.CommandLineInterfaceDriver a b c

jar_name="./target/dl4j-jar-with-dependencies.jar"
main_class_name="org.deeplearning4j.cli.driver.CommandLineInterfaceDriver"

params=""

for i in $*;
do
    params+="$i "
done


java -cp $jar_name $main_class_name $params


printf '\n\n'


